<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <extension point="org.knime.workbench.repository.categories">
      <category description="Tools for interacting with the data.world platform" icon="icons/dw_sparkle8bit16_transparent.png" level-id="dataWorld" name="data.world" path="/"/>
   </extension>
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/dataWorld" factory-class="org.knime.data.world.restful.GetDatasetInfoNodeFactory" id="org.knime.data.world.restful.GetDatasetInfoNodeFactory"/>
      <node category-path="/dataWorld" factory-class="org.knime.data.world.restful.AddDatasetFileNodeFactory" id="org.knime.data.world.restful.AddDatasetFileNodeFactory"/>
      <node category-path="/dataWorld" factory-class="org.knime.data.world.restful.DatasetFileReaderNodeFactory" id="org.knime.data.world.restful.DatasetFileReaderNodeFactory"/>
   </extension>
   
   <extension point="org.eclipse.ui.preferencePages">
      <page
            category="org.knime.workbench.ui.preferences"
            class="org.knime.data.world.prefs.DWPreferencePage"
            id="org.knime.data.world.prefs.DWPreferencePage"
            name="data.world Integration"
      />
   </extension>
   
   <extension point="org.knime.core.JDBCDriver">
      <jdbcDriver jarFile="lib/dw-jdbc-0.4.1-shaded.jar" />
   </extension>
   
</plugin>
